{
  "logging": null,
  "apps": {
    "http": {
      "servers": {
        "srv0": {
          "listen": [
            ":80"
          ],
          "routes": [
            {
              "handle": [
                {
                  "handler": "reverse_proxy",
                  "upstreams": [
                    {
                      "dial": "cloudyadminapp:8080"
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  }
}

